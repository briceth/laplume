<div class="wrapper padded mission">
  <div class="container">
      <div class="row">
        <div class="col-xs-12 col-sm-6 col-sm-offset-3">
          <div class="form">
            <%= simple_form_for([ @mission ]) do |form| %>
              <%= form.input :title %>
              <%= form.input :description %>
              <%= form.input :category %>
              <%= form.input :status %>
              <%= form.button :submit %>
            <% end %>
          </div>
        </div>
      </div>
    </div>
</div>



